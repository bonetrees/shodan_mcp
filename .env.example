# Shodan API Key
# Get your API key from https://account.shodan.io/
SHODAN_API_KEY=your_shodan_api_key_here

# Logging Configuration (optional)
# Python logging level for server-side logs (DEBUG, INFO, WARNING, ERROR)
SHODAN_MCP_LOG_LEVEL=INFO

# MCP protocol logging level - controls what gets sent to Claude (DEBUG, INFO, WARNING, ERROR)
SHODAN_MCP_PROTOCOL_LOG_LEVEL=INFO

